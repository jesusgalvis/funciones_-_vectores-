#include<stdio.h>
#include<stdlib.h>

void fun (int v[]);
void imprimir(int v[]);
int main(){
	
	int v[2];
	
	fun (v);
	imprimir (v);
	
	return 0;
	
}

void fun (int v[]){
	
	int i;
	
	for(i=0;i<2;i++){
		printf("ingrese el valor %d: ",i+1);
		scanf("%d",&v[i]);
	}
	
}

void imprimir (int v[]){
	
	int i;
	
	for(i=0;i<2;i++){
		printf("%d, ",v[i]);
	}
	
	
}
